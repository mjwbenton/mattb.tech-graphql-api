{
  "name": "@mattb.tech/graphql-api-lambda",
  "version": "1.0.0",
  "description": "Lambda function Graphql API for mattb.tech",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run external-schemas && npm run codegen && npm run typescript",
    "fix": "prettier --write '**/*' -u --ignore-path '../.gitignore'",
    "check": "tsc --noEmit && prettier --check '**/*' -u --ignore-path '../.gitignore'",
    "start": "npm run codegen:watch & npm run typescript:watch & AWS_PROFILE=api-development nodemon ./dist/devServer.js",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "external-schemas": "npm run billio-schema && npm run healthio-schema",
    "billio-schema": "ts-node ./src/bin/fetchSchemaExternal.ts https://api-readonly.billio.mattb.tech ./src/generated/billio/schema.json",
    "healthio-schema": "ts-node ./src/bin/fetchSchemaExternal.ts https://graphql.healthio.mattb.tech ./src/generated/healthio/schema.json",
    "typescript": "tsc",
    "typescript:watch": "tsc --watch"
  },
  "author": "Matt Benton <m@mattb.tech>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/typescript": "4.0.0",
    "@graphql-codegen/typescript-resolvers": "4.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/mkdirp": "^1.0.1",
    "@types/moment": "^2.13.0",
    "@types/node-fetch": "2.6.1",
    "@types/xml2js": "^0.4.8",
    "express": "^4.17.1",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@apollo/server": "^4.7.4",
    "@apollo/utils.keyvaluecache": "^3.0.0",
    "@as-integrations/aws-lambda": "^2.0.1",
    "@aws-sdk/client-dynamodb": "^3.370.0",
    "@aws-sdk/lib-dynamodb": "^3.370.0",
    "@graphql-tools/delegate": "^10.0.0",
    "@graphql-tools/merge": "^9.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "@graphql-tools/wrap": "^10.0.0",
    "@mattb.tech/graphql-api-oauth-lib": "1.0.0",
    "aws-lambda": "^1.0.6",
    "axios": "^0.21.2",
    "date-fns": "^2.29.3",
    "dotenv": "^8.2.0",
    "envalid": "^7.3.1",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-tag": "^2.12.6",
    "moment": "^2.29.2",
    "node-fetch": "2.6.7",
    "xml2js": "^0.5.0"
  }
}
