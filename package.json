{
  "name": "@mattb.tech/graphql-api",
  "version": "1.0.0",
  "description": "Graphql API for mattb.tech",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit && prettier --check '**/*'",
    "create-stack": "cf-create-stack -s mattb-tech-graphql-api -p mattb.tech-deploy -f template.yaml",
    "update-stack": "cf-deploy-stack -s mattb-tech-graphql-api -p mattb.tech-deploy -f template.yaml",
    "update-lambda": "cf-update-lambda -s mattb-tech-graphql-api -p mattb.tech-deploy -c mattb.tech-graphql-api -k graphql-api.zip -r GraphqlApi",
    "goodreads-oauth": "node ./src/bin/goodreadsOauth.js",
    "start": "npm run build && node ./src/devServer.js"
  },
  "author": "Matt Benton <m@mattb.tech>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@aws-cdk/aws-s3": "^1.22.0",
    "@aws-cdk/core": "^1.22.0",
    "@mattb.tech/cloudformation-scripts": "^2.0.1",
    "@types/aws-lambda": "^8.10.40",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.5.0",
    "@types/moment": "^2.13.0",
    "@types/node-fetch": "^2.5.4",
    "@types/oauth": "^0.9.1",
    "@types/request": "^2.48.4",
    "@types/request-promise-native": "^1.0.17",
    "@types/xml2js": "^0.4.5",
    "aws-cdk": "^1.22.0",
    "express": "^4.17.1",
    "oauth": "^0.9.15",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@mattb.tech/flickr-api": "^1.1.1",
    "apollo-datasource": "^0.6.4",
    "apollo-server-cache-dynamodb": "^2.0.1",
    "apollo-server-lambda": "^2.9.16",
    "aws-lambda": "^1.0.5",
    "aws-sdk": "^2.610.0",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "xml2js": "^0.4.23"
  }
}
