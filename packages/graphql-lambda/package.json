{
  "name": "@mattb.tech/graphql-api-lambda",
  "version": "1.0.0",
  "description": "Lambda function Graphql API for mattb.tech",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run codegen && tsc",
    "prettier": "prettier --write '**/*.ts'",
    "check": "tsc --noEmit && prettier --check '**/*.ts'",
    "goodreads-oauth": "node ./src/bin/goodreadsOauth.js",
    "igdb-test": "node ./src/bin/igdbTest.js",
    "start": "npm run build && node ./src/devServer.js",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "author": "Matt Benton <m@mattb.tech>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.1",
    "@types/aws-lambda": "^8.10.71",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/graphql": "^14.5.0",
    "@types/moment": "^2.13.0",
    "@types/node-fetch": "^2.5.8",
    "@types/oauth": "^0.9.1",
    "@types/request": "^2.48.5",
    "@types/request-promise-native": "^1.0.17",
    "@types/xml2js": "^0.4.8",
    "express": "^4.17.1",
    "oauth": "^0.9.15",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "@graphql-codegen/typescript": "1.20.2",
    "@graphql-codegen/typescript-resolvers": "1.18.1"
  },
  "dependencies": {
    "@mattb.tech/flickr-api": "^1.1.1",
    "apollo-datasource": "^0.7.3",
    "apollo-server-cache-dynamodb": "^2.0.1",
    "apollo-server-lambda": "^2.19.2",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.839.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.5.0",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "xml2js": "^0.4.23"
  }
}
